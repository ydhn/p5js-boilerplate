{
  "plugins": [
    "@babel/transform-runtime",
    "@babel/plugin-proposal-optional-chaining"
  ],
  "presets": [
    [
      "@babel/es2015",
      {
        "loose": true,
        "modules": false
      }
    ],
    "@babel/preset-stage-0",
    "@babel/preset-stage-1",
    "@babel/preset-stage-2",
    "@babel/preset-stage-3"
  ],
  "env": {
    "test": {
      "presets": [
        "@babel/es2015"
      ]
    }
  }
}